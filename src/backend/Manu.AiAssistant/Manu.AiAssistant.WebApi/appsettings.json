{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureKeyVault": {
    "Url": ""
  },
  "AzureAd": {
    "ClientId": "<Your-Client-Id>",
    "TenantId": "<Your-Tenant-Id>",
    "ClientSecret": "<Your-Client-Secret>",
    "RedirectUri": "<Your-Redirect-Uri>"
  },
  "ConnectionStrings": {
    "DefaultConnection": "<Your-Connection-String>"
  },
  "Dalle": {
    "Endpoint": "https://your-dalle-endpoint/api/generate",
    "ApiKey": "<Your-Dalle-Api-Key>"
  },
  "AzureStorage": {
    "AccountUrl": "https://<your-account>.blob.core.windows.net",
    "ContainerName": "images"
  },
  "App": {
    "PublicImageDomain": "https://ai.manucode.ar"
  },
  "ApplicationInsights": {
    "ConnectionString": "<YOUR-APPLICATION-INSIGHTS-CONNECTION-STRING>"
  },
  "CosmosDb": {
    "AccountEndpoint": "<your-cosmos-endpoint>",
    "AccountKey": "<your-cosmos-key>",
    "DatabaseName": "<your-cosmos-database>",
    "ImageGeneration": {
      "ContainerName": "<your-imagegeneration-container>"
    }
    // Add more entities here as needed
  },
  "Database": {
    "ApplyMigrationsOnStartup": false
  }
}
