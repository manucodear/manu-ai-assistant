{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureKeyVault": {
    "Url": "<Your-KeyVault-Url>"
  },
  "AzureAd": {
    "ClientId": "<Your-Client-Id>",
    "TenantId": "<Your-Tenant-Id>",
    "ClientSecret": "<Your-Client-Secret>",
    "RedirectUri": "<Your-Redirect-Uri>"
  },
  "CosmosDb": {
    "AccountEndpoint": "<Your-Cosmos-Account-Endpoint>",
    "AccountKey": "<Your-Cosmos-Account-Key>",
    "DatabaseName": "<Your-Database-Name>",
    "ContainerName": "<Your-Container-Name>",
    "ContainerPrefix": "<Your-Container-Prefix>",
    "EventContainerName": "<Your-Event-Container-Name>",
    "IsRateLimited": "false",
    "MaxItemsToReturn": 100,
    "RequestTimeout": 30
  }
}
