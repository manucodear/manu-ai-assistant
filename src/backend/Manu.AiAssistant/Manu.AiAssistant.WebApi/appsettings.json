{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureKeyVault": {
    "Url": "",
    "DataProtectionKeyId": "MasterKey"
  },
  "AzureAd": {
    "ClientId": "<Your-Client-Id>",
    "TenantId": "<Your-Tenant-Id>",
    "ClientSecret": "<Your-Client-Secret>",
    "RedirectUri": "<Your-Redirect-Uri>"
  },
  "Google": {
    "ClientId": "<Your-Google-Client-Id>",
    "ClientSecret": "<Your-Google-Client-Secret>",
    "RedirectUri": "<Your-Google-Redirect-Uri>"
  },
  "Dalle": {
    "Endpoint": "https://your-dalle-endpoint/api/generate",
    "ApiKey": "<Your-Dalle-Api-Key>"
  },
  "AzureStorage": {
    "AccountUrl": "https://<your-account>.blob.core.windows.net",
    "ContainerName": "images"
  },
  "App": {
    "PublicImageDomain": "https://ai.manucode.ar",
    "ImagePath": "https://ai.manucode.ar/api/image",
    "JwtKey": "your-very-strong-random-secret-key"
  },
  "ApplicationInsights": {
    "ConnectionString": "<YOUR-APPLICATION-INSIGHTS-CONNECTION-STRING>"
  },
  "CosmosDb": {
    "AccountEndpoint": "<your-cosmos-endpoint>",
    "AccountKey": "<your-cosmos-key>",
    "DatabaseName": "<your-cosmos-database>",
    "IdentityDatabaseName": "<your-identity-database>",
    "Containers": {
      "Image": "<your-images-container>",
      "Chat": "<your-chats-container>"
    }
  },
  "Chat": {
    "Endpoint": "https://manu-assistant-open-ai.openai.azure.com/openai/v1/",
    "ApiKey": "<your-api-key>",
    "DeploymentName": "gpt-4o"
  }
  // Add more entities here as needed
}
